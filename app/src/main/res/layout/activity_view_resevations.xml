<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.satu.obiektysportowe.ViewReservationsActivity"
    android:background="@drawable/boisko2">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Moje Rezerwacje"
        android:id="@+id/textView_myResevations"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:id="@+id/expandableListView_reservations"
        android:layout_marginBottom="208dp"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



</RelativeLayout>